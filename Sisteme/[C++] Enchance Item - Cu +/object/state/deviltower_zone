deviltower_zone={["start"]=0,get_4floor_stone_pos= function ()local positions = 
{ 
{ 368 , 629 } , { 419 , 630 } , { 428 , 653 } , { 422 , 679 } , 
{ 395 , 689 } , { 369 , 679 } , { 361 , 658 } , 
} 
for i = 1 , 6 begin 
local j = number ( i , 7 ) 
if i ~= j then 
local t = positions [ i ] ; 
positions [ i ] = positions [ j ] ; 
positions [ j ] = t ; 
end 
end 
return positions 
end }